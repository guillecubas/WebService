<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.apache.org/ode/schemas/dd/2007/03 deployment.xsd">
    <process name="PizzaOrderProcess">
        <active>true</active>
        <retired>false</retired>
        <process-events generate="all"/>
        <provide partnerLink="PizzaPricePL">
            <service name="PizzaPriceService"/>
        </provide>
        <provide partnerLink="PizzaPopularityPL">
            <service name="PizzaPopularityService"/>
        </provide>
        <invoke partnerLink="OrderProcessingPL">
            <service name="OrderProcessingService"/>
        </invoke>
    </process>
</deploy>
